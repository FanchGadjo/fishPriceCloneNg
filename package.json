{
  "name": "angular-tailwind-starter",
  "version": "0.0.1",
  "scripts": {
    "analyze": "webpack-bundle-analyzer dist/meraki-pipeline/stats-es2015.json",
    "ng": "ng",
    "start": "npm run tailwind && ng serve -o",
    "build": "npm run tailwind && npm run purge && ng build",
    "build:stats": "npm run tailwind && npm run purge && ng build --stats-json",
    "build:analyze": "npm run build:stats && npm run analyze",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress": "cypress open",
    "purge": "purgecss -c ./purgecss.config.js -o src/",
    "tailwind": "./node_modules/.bin/tailwind build ./src/tailwind-build.css -c ./tailwind.config.js -o ./src/styles.tailwind.css",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "yarn docs:json && start-storybook -p 6006",
    "build-storybook": "yarn docs:json && build-storybook"
  },
  "dependencies": {
    "@angular/animations": "^8.2.2",
    "@angular/common": "^8.2.2",
    "@angular/compiler": "^8.2.2",
    "@angular/core": "^8.2.2",
    "@angular/forms": "^8.2.2",
    "@angular/platform-browser": "^8.2.2",
    "@angular/platform-browser-dynamic": "^8.2.2",
    "@angular/router": "^8.2.2",
    "core-js": "^3.2.1",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.1",
    "@angular/cli": "~8.2.1",
    "@angular/compiler-cli": "^8.2.2",
    "@angular/language-service": "^8.2.2",
    "@babel/core": "^7.11.6",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/angular": "^6.0.21",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.7.1",
    "babel-loader": "^8.1.0",
    "codelyzer": "^5.0.1",
    "cypress": "^5.1.0",
    "husky": "^3.0.3",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "protractor": "~5.4.0",
    "purgecss": "^1.3.0",
    "react-is": "^16.13.1",
    "tailwindcss": "^1.1.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
